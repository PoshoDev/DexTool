{
    global.current_box--;
    
    if (global.current_box &lt; 1)
        global.current_box = 1;
    else  
        scroll_do();
}
else if (mouse_wheel_down())
{
    global.current_box++;
    
    if (global.current_box &gt; global.box_count-1)
        global.current_box = global.box_count-1;
    else
        scroll_do();
}
    

